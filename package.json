{
  "name": "firebird",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon ./src/index.js",
    "cron": "node ./src/cron.js",
    "start": "pm2 start --name egide_integration ./src/cron.js",
    "test": "node",
    "extract": "node ./src/__tests__/extract.test.js",
    "upload": "node ./src/__tests__/upload.test.js",
    "destroy": "node ./src/destroyAll.js",
    "db:migrate": "sequelize db:migrate",
    "startover": "rm ./db/db.sqlite && sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "form-data": "^4.0.0",
    "formidable": "^2.0.1",
    "lodash": "^4.17.21",
    "node-cron": "^3.0.1",
    "node-firebird": "^1.0.0",
    "sequelize": "^6.21.0",
    "sqlite3": "^5.0.8"
  },
  "devDependencies": {
    "nodemon": "^2.0.18",
    "sequelize-cli": "^6.4.1"
  }
}
